<%- include('./partials/head')%>
<%- include('./partials/nav')%>

<table class="table table-striped table-hover" style="margin-bottom: 100px;">
        <thead>
            <th>Book</th>
            <th>Title</th>
            <th>Price</th>
            <th></th>
            
        </thead>
    <tbody>
    <% for(b of booksjson){ %>
        <tr>
            <td><img src="<%= b.image %>" alt="<%= b.book %>" style="width: 100px; height: auto;"></td> 
            <td style="vertical-align: middle;"><%= b.title %></td>
            <td style="vertical-align: middle;"><%= b.price %></td>
            <td style="vertical-align: middle;">Add to Cart</td>
        </tr>
        <% } %>
    </tbody>
</table>



<%- include('./partials/footer')%>